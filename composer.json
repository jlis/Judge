{
  "name": "jlis/judge",
  "description": "A feature and value decider for Laravel 5",
  "keywords": [
    "laravel",
    "feature",
    "value",
    "toggle",
    "decision",
    "decider",
    "judge",
    "featuretoggle"
  ],
  "homepage": "http://github.com/jlis/judge",
  "license": "MIT",
  "authors": [
    {
      "name": "Julius Ehrlich",
      "email": "julius@ehrlich-bros.de"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/expression-language": "~2.7",
    "illuminate/support": "5.*",
    "mockery/mockery": "^1.0@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "symfony/var-dumper": "~2.7"
  },
  "autoload": {
    "psr-0": {
      "jlis\\Judge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "jlis\\Tests\\Judge\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
