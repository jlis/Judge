{
  "name": "jlis/judge",
  "description": "Judge handles feature and value toggles/flaggings for Laravel.",
  "keywords": [
    "laravel",
    "feature",
    "value",
    "toggle",
    "decision",
    "decider",
    "judge",
    "featuretoggle",
    "featureflag"
  ],
  "homepage": "http://github.com/jlis/judge",
  "license": "MIT",
  "authors": [
    {
      "name": "Julius Ehrlich",
      "email": "julius@ehrlich-bros.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jlis\\Judge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jlis\\Tests\\Judge\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6.0",
    "symfony/expression-language": "~2.7|~3.0",
    "illuminate/support": "5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "^0.9.5",
    "scrutinizer/ocular": "~1.3"
  },
  "suggest": {
    "predis/predis": "Allows reading the configuration from Redis"
  },
  "scripts": {
    "test": "phpunit"
  }
}
